<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.meha.myapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Welcome"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape">
        </activity>

        <activity
            android:name=".Simulation"
            android:theme="@style/SimulationTheme"
            android:label="@string/title_activity_simulation"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".theoryActivity"
            android:theme="@style/AlgoTheme"
            android:label="@string/title_activity_theory"
            android:screenOrientation="landscape">
        </activity>
        <activity
            android:name=".Credits"
            android:theme="@style/AboutTheme"
            android:label="@string/title_activity_credits"
            android:screenOrientation="landscape">
        </activity>
        <activity
            android:name=".InputActivity"
            android:theme="@style/InputTheme"
            android:label="@string/title_activity_input"
            android:screenOrientation="landscape">
        </activity>
        <activity
            android:name=".dataChanger"
            android:label="@string/title_activity_data_changer"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="landscape">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.meha.myapp.MainActivity" />
        </activity>
    </application>

</manifest>
